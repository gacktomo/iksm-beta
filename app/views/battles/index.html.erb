<p id="notice"><%= notice %></p>

<h1>Battles</h1>

<table>
  <thead>
    <tr>
      <th>Result</th>
      <th>Kill</th>
      <th>Death</th>
      <th>Assist</th>
      <th>Special</th>
      <th>Weapon</th>
      <th>Udemae</th>
      <th>Stage</th>
      <th>Rule</th>
      <th>User</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @battles.each do |battle| %>
      <tr>
        <td><%= battle.result %></td>
        <td><%= battle.kill %></td>
        <td><%= battle.death %></td>
        <td><%= battle.assist %></td>
        <td><%= battle.special %></td>
        <td><%= battle.weapon_id %></td>
        <td><%= battle.udemae %></td>
        <td><%= battle.stage_id %></td>
        <td><%= battle.rule %></td>
        <td><%= battle.user_id %></td>
        <td><%= link_to 'Show', battle %></td>
        <td><%= link_to 'Edit', edit_battle_path(battle) %></td>
        <td><%= link_to 'Destroy', battle, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Battle', new_battle_path %>
